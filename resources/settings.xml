<settings>
<category label="Title Search">

	<setting label="Custom Prefix for adding to Favourites" type="text" id="prefix" default=""/>

	<setting label="Title" type="text" id="title" default=""/>
	
	<setting label="IMDb Server" type="select" id="server" default="www" values="akas|www"/>
	<setting label="Results Per Page" type="select" id="count" default="50" values="50|100"/>

	<setting label="Type" type="select" id="title_type" default="feature" values="feature|tv_movie|tv_series|tv_episode|tv_special|mini_series|documentary|game|short|video"/>

	<setting label="Year Start" type="slider" id="release_date_start" subsetting="true" default="2015" range="1900,1,2016" option="int" />
	<setting label="Year End" type="slider" id="release_date_end" subsetting="true" default="2016" range="1900,1,2016" option="int" />

	<setting label="User Rating Low" type="slider" id="user_rating_low" subsetting="true" default="6.0" range="0.0,0.1,10.0" option="float" />
	<setting label="User Rating High" type="slider" id="user_rating_high" subsetting="true" default="10.0" range="0.0,0.1,10.0" option="float" />

	<setting label="Votes Minimum" type="number" id="num_votes_low" default="100"/>
	<setting label="Votes Maximum" type="number" id="num_votes_high" default=""/>

	<setting label="Extra Genre" type="select" id="genres" values="Any|action|adventure|animation|biography|comedy|crime|documentary|drama|family|fantasy|film_noir|game_show|history|horror|music|musical|mystery|news|reality_tv|romance|sci_fi|sport|talk_show|thriller|war|western"/>

	<setting label="Awards" type="select" id="groups" values="Any|top_100|top_250|top_1000|now-playing-us|oscar_winners|oscar_best_picture_winners|oscar_best_director_winners|oscar_nominees|emmy_winners|emmy_nominees|golden_globe_winners|golden_globe_nominees|razzie_winners|razzie_nominees|national_film_registry|bottom_100|bottom_250|bottom_1000"/>

	<setting label="Company" type="select" id="companies" values="Any|fox|columbia|dreamworks|mgm|paramount|universal|disney|warner"/>

	<setting label="Box Office Gross US Minimum" type="number" id="boxoffice_gross_us_low" default=""/>
